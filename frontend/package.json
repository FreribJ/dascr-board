{
  "name": "dose",
  "version": "1.0.0",
  "description": "A go based darts scoretracking app",
  "license": "MIT",
  "author": {
    "name": "Patrick Hener",
    "email": "patrickhener@gmx.de",
    "url": "https://github.com/dascr/dascr-board"
  },
  "scripts": {
    "build": "routify -b && rollup -c",
    "dev": "run-p routify rollup",
    "dev-expose": "run-p routify rollup-expose",
    "routify": "routify",
    "rollup": "rollup -c -w",
    "rollup-expose": "HOST=0.0.0.0 rollup -c -w",
    "start": "sirv public --single",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@roxi/routify": "^2.7.3",
    "autoprefixer": "^10.0.4",
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.14",
    "postcss-load-config": "^3.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^1.4.2",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.6.1",
    "tailwindcss": "^2.0.1"
  },
  "routify": {
    "extensions": "svelte,html,svx,md",
    "debug": true
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@zerodevx/svelte-toast": "^0.1.3",
    "crypto-js": "^4.0.0",
    "ky": "^0.25.0",
    "sirv-cli": "^1.0.0",
    "svelte-easy-crop": "^1.0.3"
  }
}
